/*Bubble sort*/

#include <stdio.h>

int main()
{
  int a[10]={ 14,17,18,53,2,89,45,29,53,45};
  int i,k,z;
  for(i=0;i<10;i++){
      for(k=0;k<10-i;k++){
          if(a[k]>a[k+1]){
              z=a[k];
              a[k]=a[k+1];
              a[k+1]=z;
              
          }
      }
  }
   printf(" The sorted array is");
  for(i=0;i<10;i++){
      
      printf(" %d ",a[i]);
  }
    return 0;
}
